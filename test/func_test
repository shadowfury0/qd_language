a = 3
fun abc():
	global a = a * a
	if a > 10000:
		ret 1
	pass
	abc()
pass

abc()


a = 10
fun dd():
	global a = a - 1
	if a < 0:
		ret 1
	pass
	ret dd() * 2
pass

p = dd()
